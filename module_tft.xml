  <patternset id="excluded.from.module.tft">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.tft">
    <patternset refid="excluded.from.module.tft"/>
  </patternset>
  
  <path id="tft.module.sourcepath">
    <dirset dir="${module.tft.basedir}">
      <include name="src/main/java"/>
      <include name="src/main/resources"/>
    </dirset>
  </path>
  
  <path id="tft.module.test.sourcepath">
    <dirset dir="${module.tft.basedir}">
      <include name="src/test/java"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.tft" depends="compile.module.tft.production,compile.module.tft.tests" description="Compile module tft"/>
  
  <target name="compile.module.tft.production" depends="register.custom.compilers" description="Compile module tft; production classes">
    <mkdir dir="${tft.output.dir}"/>
    <javac2 destdir="${tft.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.tft}/javac">
      <compilerarg line="${compiler.args.tft}"/>
      <bootclasspath refid="tft.module.bootclasspath"/>
      <classpath refid="tft.module.production.classpath"/>
      <src refid="tft.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.tft"/>
    </javac2>
    
    <copy todir="${tft.output.dir}">
      <fileset dir="${module.tft.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.tft.basedir}/src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.tft.tests" depends="register.custom.compilers,compile.module.tft.production" description="compile module tft; test classes" unless="skip.tests">
    <mkdir dir="${tft.testoutput.dir}"/>
    <javac2 destdir="${tft.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.tft}/javac">
      <compilerarg line="${compiler.args.tft}"/>
      <bootclasspath refid="tft.module.bootclasspath"/>
      <classpath refid="tft.module.classpath"/>
      <src refid="tft.module.test.sourcepath"/>
      <patternset refid="excluded.from.compilation.tft"/>
    </javac2>
    
    <copy todir="${tft.testoutput.dir}">
      <fileset dir="${module.tft.basedir}/src/test/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="clean.module.tft" description="cleanup module">
    <delete dir="${tft.output.dir}"/>
    <delete dir="${tft.testoutput.dir}"/>
  </target>
